{
  "type": "object",
  "required": ["stakeholder_mapping", "relationship_assessment", "influence_analysis", "engagement_strategy", "citations"],
  "properties": {
    "stakeholder_mapping": {
      "type": "object",
      "required": ["primary_stakeholders", "secondary_stakeholders", "stakeholder_power_grid"],
      "properties": {
        "primary_stakeholders": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": ["stakeholder", "type", "influence_level", "interest_level"],
            "properties": {
              "stakeholder": {"type": "string"},
              "type": {
                "type": "string",
                "enum": ["investor", "customer", "employee", "regulator", "supplier", "community", "partner", "government"]
              },
              "influence_level": {
                "type": "string",
                "enum": ["very_high", "high", "medium", "low"]
              },
              "interest_level": {
                "type": "string",
                "enum": ["very_high", "high", "medium", "low"]
              },
              "key_concerns": {
                "type": "array",
                "items": {"type": "string"}
              },
              "decision_making_role": {"type": "string"}
            }
          }
        },
        "secondary_stakeholders": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["stakeholder", "type", "relevance"],
            "properties": {
              "stakeholder": {"type": "string"},
              "type": {
                "type": "string",
                "enum": ["media", "advocacy_groups", "industry_associations", "competitors", "local_community", "unions"]
              },
              "relevance": {
                "type": "string",
                "enum": ["high", "medium", "low"]
              }
            }
          }
        },
        "stakeholder_power_grid": {
          "type": "object",
          "required": ["champions", "supporters", "neutral", "critics"],
          "properties": {
            "champions": {
              "type": "array",
              "items": {"type": "string"}
            },
            "supporters": {
              "type": "array",
              "items": {"type": "string"}
            },
            "neutral": {
              "type": "array",
              "items": {"type": "string"}
            },
            "critics": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        }
      }
    },
    "relationship_assessment": {
      "type": "object",
      "required": ["relationship_quality", "trust_levels", "communication_effectiveness"],
      "properties": {
        "relationship_quality": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["stakeholder", "quality", "trend", "key_issues"],
            "properties": {
              "stakeholder": {"type": "string"},
              "quality": {
                "type": "string",
                "enum": ["excellent", "good", "fair", "poor", "adversarial"]
              },
              "trend": {
                "type": "string",
                "enum": ["improving", "stable", "deteriorating"]
              },
              "key_issues": {
                "type": "array",
                "items": {"type": "string"}
              },
              "last_interaction": {"type": "string"}
            }
          }
        },
        "trust_levels": {
          "type": "object",
          "required": ["overall_trust", "credibility_factors"],
          "properties": {
            "overall_trust": {
              "type": "string",
              "enum": ["high", "moderate", "low", "very_low"]
            },
            "credibility_factors": {
              "type": "array",
              "items": {"type": "string"}
            },
            "trust_building_initiatives": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "communication_effectiveness": {
          "type": "object",
          "required": ["channels", "frequency", "feedback_quality"],
          "properties": {
            "channels": {
              "type": "array",
              "items": {"type": "string"}
            },
            "frequency": {
              "type": "string",
              "enum": ["continuous", "regular", "periodic", "ad_hoc", "insufficient"]
            },
            "feedback_quality": {
              "type": "string",
              "enum": ["excellent", "good", "adequate", "poor"]
            },
            "message_clarity": {
              "type": "string",
              "enum": ["very_clear", "clear", "somewhat_clear", "unclear"]
            }
          }
        }
      }
    },
    "influence_analysis": {
      "type": "object",
      "required": ["power_structures", "decision_influencers", "coalition_potential"],
      "properties": {
        "power_structures": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["structure", "key_players", "dynamics"],
            "properties": {
              "structure": {"type": "string"},
              "key_players": {
                "type": "array",
                "items": {"type": "string"}
              },
              "dynamics": {"type": "string"},
              "stability": {
                "type": "string",
                "enum": ["very_stable", "stable", "changing", "volatile"]
              }
            }
          }
        },
        "decision_influencers": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["influencer", "scope", "methods"],
            "properties": {
              "influencer": {"type": "string"},
              "scope": {"type": "string"},
              "methods": {
                "type": "array",
                "items": {"type": "string"}
              },
              "effectiveness": {
                "type": "string",
                "enum": ["very_effective", "effective", "moderately_effective", "ineffective"]
              }
            }
          }
        },
        "coalition_potential": {
          "type": "object",
          "required": ["natural_allies", "potential_partners", "opposition_groups"],
          "properties": {
            "natural_allies": {
              "type": "array",
              "items": {"type": "string"}
            },
            "potential_partners": {
              "type": "array",
              "items": {"type": "string"}
            },
            "opposition_groups": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        }
      }
    },
    "engagement_strategy": {
      "type": "object",
      "required": ["engagement_approach", "prioritization", "resource_allocation"],
      "properties": {
        "engagement_approach": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["stakeholder", "strategy", "tactics", "success_metrics"],
            "properties": {
              "stakeholder": {"type": "string"},
              "strategy": {"type": "string"},
              "tactics": {
                "type": "array",
                "items": {"type": "string"}
              },
              "success_metrics": {
                "type": "array",
                "items": {"type": "string"}
              },
              "timeline": {"type": "string"}
            }
          }
        },
        "prioritization": {
          "type": "object",
          "required": ["high_priority", "medium_priority", "low_priority"],
          "properties": {
            "high_priority": {
              "type": "array",
              "items": {"type": "string"}
            },
            "medium_priority": {
              "type": "array",
              "items": {"type": "string"}
            },
            "low_priority": {
              "type": "array",
              "items": {"type": "string"}
            },
            "prioritization_criteria": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "resource_allocation": {
          "type": "object",
          "required": ["budget", "personnel", "time_investment"],
          "properties": {
            "budget": {"type": "string"},
            "personnel": {"type": "string"},
            "time_investment": {"type": "string"},
            "roi_expectations": {"type": "string"}
          }
        }
      }
    },
    "conflict_management": {
      "type": "object",
      "properties": {
        "current_conflicts": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["conflict", "parties", "severity"],
            "properties": {
              "conflict": {"type": "string"},
              "parties": {
                "type": "array",
                "items": {"type": "string"}
              },
              "severity": {
                "type": "string",
                "enum": ["critical", "high", "medium", "low"]
              },
              "resolution_approach": {"type": "string"}
            }
          }
        },
        "resolution_mechanisms": {
          "type": "array",
          "items": {"type": "string"}
        }
      }
    },
    "citations": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["source_id"],
        "properties": {
          "source_id": {"type": "integer"},
          "quote": {"type": "string"},
          "page": {"type": "string"},
          "url": {"type": "string"}
        }
      }
    }
  }
}