{
  "type": "object",
  "required": ["innovation_assessment", "rd_capabilities", "digital_transformation", "innovation_pipeline", "citations"],
  "properties": {
    "innovation_assessment": {
      "type": "object",
      "required": ["innovation_maturity", "innovation_strategy", "innovation_culture"],
      "properties": {
        "innovation_maturity": {
          "type": "string",
          "enum": ["leading", "advanced", "developing", "emerging", "lagging"]
        },
        "innovation_strategy": {
          "type": "object",
          "required": ["focus_areas", "investment_level", "time_horizon"],
          "properties": {
            "focus_areas": {
              "type": "array",
              "minItems": 1,
              "items": {"type": "string"}
            },
            "investment_level": {
              "type": "string",
              "enum": ["high", "moderate", "low", "insufficient"]
            },
            "time_horizon": {
              "type": "string",
              "enum": ["short_term", "medium_term", "long_term", "balanced"]
            },
            "strategic_alignment": {
              "type": "string",
              "enum": ["fully_aligned", "mostly_aligned", "partially_aligned", "misaligned"]
            }
          }
        },
        "innovation_culture": {
          "type": "object",
          "required": ["risk_tolerance", "experimentation", "collaboration"],
          "properties": {
            "risk_tolerance": {
              "type": "string",
              "enum": ["high", "moderate", "low", "risk_averse"]
            },
            "experimentation": {
              "type": "string",
              "enum": ["encouraged", "tolerated", "limited", "discouraged"]
            },
            "collaboration": {
              "type": "string",
              "enum": ["cross_functional", "departmental", "limited", "siloed"]
            },
            "failure_acceptance": {
              "type": "string",
              "enum": ["learning_opportunity", "acceptable", "tolerated", "punished"]
            }
          }
        }
      }
    },
    "rd_capabilities": {
      "type": "object",
      "required": ["rd_investment", "research_infrastructure", "talent_pool", "partnerships"],
      "properties": {
        "rd_investment": {
          "type": "object",
          "required": ["annual_spend", "percentage_of_revenue", "trend"],
          "properties": {
            "annual_spend": {"type": "string"},
            "percentage_of_revenue": {"type": "number"},
            "trend": {
              "type": "string",
              "enum": ["increasing", "stable", "decreasing"]
            },
            "allocation_breakdown": {
              "type": "array",
              "items": {
                "type": "object",
                "required": ["category", "percentage"],
                "properties": {
                  "category": {"type": "string"},
                  "percentage": {"type": "number"}
                }
              }
            }
          }
        },
        "research_infrastructure": {
          "type": "object",
          "required": ["facilities", "equipment", "capabilities"],
          "properties": {
            "facilities": {
              "type": "array",
              "items": {"type": "string"}
            },
            "equipment": {"type": "string"},
            "capabilities": {
              "type": "array",
              "items": {"type": "string"}
            },
            "technology_platforms": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "talent_pool": {
          "type": "object",
          "required": ["team_size", "expertise_areas", "recruitment_challenges"],
          "properties": {
            "team_size": {"type": "string"},
            "expertise_areas": {
              "type": "array",
              "items": {"type": "string"}
            },
            "recruitment_challenges": {
              "type": "array",
              "items": {"type": "string"}
            },
            "retention_rate": {"type": "string"},
            "skill_gaps": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "partnerships": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["partner", "type", "focus_area"],
            "properties": {
              "partner": {"type": "string"},
              "type": {
                "type": "string",
                "enum": ["university", "research_institute", "startup", "corporate", "government"]
              },
              "focus_area": {"type": "string"},
              "collaboration_model": {"type": "string"},
              "success_metrics": {"type": "string"}
            }
          }
        }
      }
    },
    "digital_transformation": {
      "type": "object",
      "required": ["digital_maturity", "technology_adoption", "digital_initiatives"],
      "properties": {
        "digital_maturity": {
          "type": "string",
          "enum": ["digital_native", "advanced", "developing", "traditional", "legacy"]
        },
        "technology_adoption": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["technology", "adoption_stage", "business_impact"],
            "properties": {
              "technology": {"type": "string"},
              "adoption_stage": {
                "type": "string",
                "enum": ["production", "pilot", "evaluation", "planning", "not_considered"]
              },
              "business_impact": {
                "type": "string",
                "enum": ["transformational", "significant", "moderate", "minimal"]
              },
              "investment_level": {"type": "string"}
            }
          }
        },
        "digital_initiatives": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["initiative", "status", "expected_outcome"],
            "properties": {
              "initiative": {"type": "string"},
              "status": {
                "type": "string",
                "enum": ["completed", "in_progress", "planning", "on_hold", "cancelled"]
              },
              "expected_outcome": {"type": "string"},
              "timeline": {"type": "string"},
              "budget": {"type": "string"}
            }
          }
        },
        "digital_capabilities": {
          "type": "object",
          "properties": {
            "data_analytics": {
              "type": "string",
              "enum": ["advanced", "intermediate", "basic", "limited"]
            },
            "automation": {
              "type": "string",
              "enum": ["extensive", "moderate", "limited", "minimal"]
            },
            "cloud_adoption": {
              "type": "string",
              "enum": ["cloud_native", "hybrid", "partial", "on_premise"]
            }
          }
        }
      }
    },
    "innovation_pipeline": {
      "type": "object",
      "required": ["project_portfolio", "development_process", "commercialization"],
      "properties": {
        "project_portfolio": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["project", "stage", "potential_impact", "timeline"],
            "properties": {
              "project": {"type": "string"},
              "stage": {
                "type": "string",
                "enum": ["ideation", "concept", "development", "testing", "pre_launch", "launched"]
              },
              "potential_impact": {
                "type": "string",
                "enum": ["breakthrough", "significant", "incremental", "minor"]
              },
              "timeline": {"type": "string"},
              "resource_requirement": {"type": "string"},
              "risk_level": {
                "type": "string",
                "enum": ["high", "medium", "low"]
              }
            }
          }
        },
        "development_process": {
          "type": "object",
          "required": ["methodology", "stage_gates", "decision_criteria"],
          "properties": {
            "methodology": {"type": "string"},
            "stage_gates": {
              "type": "array",
              "items": {"type": "string"}
            },
            "decision_criteria": {
              "type": "array",
              "items": {"type": "string"}
            },
            "cycle_time": {"type": "string"}
          }
        },
        "commercialization": {
          "type": "object",
          "required": ["success_rate", "time_to_market", "market_adoption"],
          "properties": {
            "success_rate": {"type": "string"},
            "time_to_market": {"type": "string"},
            "market_adoption": {"type": "string"},
            "revenue_from_new_products": {"type": "string"}
          }
        }
      }
    },
    "external_innovation": {
      "type": "object",
      "properties": {
        "open_innovation": {
          "type": "object",
          "properties": {
            "approach": {"type": "string"},
            "external_sources": {
              "type": "array",
              "items": {"type": "string"}
            },
            "collaboration_models": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "ecosystem_participation": {
          "type": "array",
          "items": {"type": "string"}
        },
        "intellectual_property": {
          "type": "object",
          "properties": {
            "patent_portfolio": {"type": "string"},
            "licensing_activity": {"type": "string"},
            "ip_strategy": {"type": "string"}
          }
        }
      }
    },
    "citations": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["source_id"],
        "properties": {
          "source_id": {"type": "integer"},
          "quote": {"type": "string"},
          "page": {"type": "string"},
          "url": {"type": "string"}
        }
      }
    }
  }
}