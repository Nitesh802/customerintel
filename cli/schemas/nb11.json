{
  "type": "object",
  "required": ["leadership_assessment", "organizational_culture", "talent_management", "succession_planning", "citations"],
  "properties": {
    "leadership_assessment": {
      "type": "object",
      "required": ["leadership_team", "governance_structure", "leadership_effectiveness"],
      "properties": {
        "leadership_team": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": ["name", "role", "tenure", "background"],
            "properties": {
              "name": {"type": "string"},
              "role": {"type": "string"},
              "tenure": {"type": "string"},
              "background": {"type": "string"},
              "key_achievements": {
                "type": "array",
                "items": {"type": "string"}
              },
              "leadership_style": {
                "type": "string",
                "enum": ["transformational", "transactional", "servant", "authentic", "democratic", "autocratic"]
              }
            }
          }
        },
        "governance_structure": {
          "type": "object",
          "required": ["board_composition", "independence", "diversity"],
          "properties": {
            "board_composition": {"type": "string"},
            "independence": {
              "type": "string",
              "enum": ["fully_independent", "majority_independent", "partially_independent", "controlled"]
            },
            "diversity": {
              "type": "object",
              "properties": {
                "gender_diversity": {"type": "string"},
                "experience_diversity": {"type": "string"},
                "skills_matrix": {
                  "type": "array",
                  "items": {"type": "string"}
                }
              }
            },
            "committee_structure": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "leadership_effectiveness": {
          "type": "object",
          "required": ["decision_making", "communication", "vision_clarity"],
          "properties": {
            "decision_making": {
              "type": "string",
              "enum": ["excellent", "good", "adequate", "poor"]
            },
            "communication": {
              "type": "string",
              "enum": ["excellent", "good", "adequate", "poor"]
            },
            "vision_clarity": {
              "type": "string",
              "enum": ["very_clear", "clear", "somewhat_clear", "unclear"]
            },
            "stakeholder_relationships": {
              "type": "string",
              "enum": ["strong", "good", "adequate", "weak"]
            }
          }
        }
      }
    },
    "organizational_culture": {
      "type": "object",
      "required": ["culture_assessment", "values", "employee_engagement"],
      "properties": {
        "culture_assessment": {
          "type": "object",
          "required": ["cultural_type", "strength", "alignment"],
          "properties": {
            "cultural_type": {
              "type": "string",
              "enum": ["innovative", "collaborative", "competitive", "customer-centric", "hierarchical", "entrepreneurial"]
            },
            "strength": {
              "type": "string",
              "enum": ["very_strong", "strong", "moderate", "weak"]
            },
            "alignment": {
              "type": "string",
              "enum": ["fully_aligned", "mostly_aligned", "partially_aligned", "misaligned"]
            }
          }
        },
        "values": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": ["value", "demonstration"],
            "properties": {
              "value": {"type": "string"},
              "demonstration": {"type": "string"},
              "employee_adoption": {
                "type": "string",
                "enum": ["high", "moderate", "low"]
              }
            }
          }
        },
        "employee_engagement": {
          "type": "object",
          "required": ["engagement_score", "retention_rate", "satisfaction_drivers"],
          "properties": {
            "engagement_score": {"type": "string"},
            "retention_rate": {"type": "string"},
            "satisfaction_drivers": {
              "type": "array",
              "items": {"type": "string"}
            },
            "culture_initiatives": {
              "type": "array",
              "items": {
                "type": "object",
                "required": ["initiative", "status", "impact"],
                "properties": {
                  "initiative": {"type": "string"},
                  "status": {
                    "type": "string",
                    "enum": ["planned", "in_progress", "completed", "on_hold"]
                  },
                  "impact": {
                    "type": "string",
                    "enum": ["high", "medium", "low"]
                  }
                }
              }
            }
          }
        }
      }
    },
    "talent_management": {
      "type": "object",
      "required": ["recruitment_strategy", "development_programs", "performance_management"],
      "properties": {
        "recruitment_strategy": {
          "type": "object",
          "required": ["talent_acquisition", "diversity_hiring", "employer_brand"],
          "properties": {
            "talent_acquisition": {"type": "string"},
            "diversity_hiring": {
              "type": "string",
              "enum": ["leading", "competitive", "basic", "limited"]
            },
            "employer_brand": {
              "type": "string",
              "enum": ["strong", "good", "moderate", "weak"]
            },
            "key_challenges": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "development_programs": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["program", "target_audience", "effectiveness"],
            "properties": {
              "program": {"type": "string"},
              "target_audience": {"type": "string"},
              "effectiveness": {
                "type": "string",
                "enum": ["highly_effective", "effective", "moderately_effective", "ineffective"]
              },
              "participation_rate": {"type": "string"}
            }
          }
        },
        "performance_management": {
          "type": "object",
          "required": ["review_process", "goal_setting", "feedback_culture"],
          "properties": {
            "review_process": {"type": "string"},
            "goal_setting": {
              "type": "string",
              "enum": ["structured", "flexible", "basic", "informal"]
            },
            "feedback_culture": {
              "type": "string",
              "enum": ["continuous", "regular", "periodic", "limited"]
            }
          }
        }
      }
    },
    "succession_planning": {
      "type": "object",
      "required": ["readiness", "key_positions", "development_pipeline"],
      "properties": {
        "readiness": {
          "type": "string",
          "enum": ["well_prepared", "adequately_prepared", "partially_prepared", "unprepared"]
        },
        "key_positions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["position", "succession_depth", "timeline"],
            "properties": {
              "position": {"type": "string"},
              "succession_depth": {
                "type": "string",
                "enum": ["deep_bench", "adequate_options", "limited_options", "no_identified_successors"]
              },
              "timeline": {"type": "string"},
              "development_required": {"type": "string"}
            }
          }
        },
        "development_pipeline": {
          "type": "object",
          "required": ["high_potential_programs", "leadership_development"],
          "properties": {
            "high_potential_programs": {"type": "string"},
            "leadership_development": {"type": "string"},
            "external_readiness": {
              "type": "string",
              "enum": ["strong", "adequate", "limited"]
            }
          }
        }
      }
    },
    "diversity_and_inclusion": {
      "type": "object",
      "properties": {
        "diversity_metrics": {
          "type": "object",
          "properties": {
            "leadership_diversity": {"type": "string"},
            "workforce_diversity": {"type": "string"},
            "pay_equity": {"type": "string"}
          }
        },
        "inclusion_initiatives": {
          "type": "array",
          "items": {"type": "string"}
        },
        "progress_tracking": {"type": "string"}
      }
    },
    "citations": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["source_id"],
        "properties": {
          "source_id": {"type": "integer"},
          "quote": {"type": "string"},
          "page": {"type": "string"},
          "url": {"type": "string"}
        }
      }
    }
  }
}