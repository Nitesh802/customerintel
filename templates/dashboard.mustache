  <h3>Customer Intelligence Reports</h3>
  </div>
</header>



<div class="local_customerintel">


  <div class="mb-3">

    <!--a class="btn btn-secondary" href="{{reportsurl}}">{{#str}} viewreports, local_customerintel {{/str}}</a-->
   <div class="dropdown">
    <a class="dropdown-toggle" href="#" role="button" id="settingsMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" title="{{#str}} settings, core {{/str}}">
        <i class="fa-light fa-gear"></i>
    </a>
    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="settingsMenu">
        <a class="dropdown-item" href="{{sourcesurl}}">{{#str}} managesources, local_customerintel {{/str}}</a>
        {{#canviewlogs}}
        <a class="dropdown-item" href="{{logsurl}}">{{#str}} viewlogs, local_customerintel {{/str}}</a>
        {{/canviewlogs}}
    </div>
</div>
  </div>
  <div class="intelcard_row">
    <div class="custintel_card"><i class="fa-thin fa-clock" style="color: #969696"></i><strong>{{queued}}</strong><div>{{#str}} queued, local_customerintel {{/str}}</div></div>
    <div class="custintel_card"><i class="fa-thin fa-circle-notch" style="color: #00BAFF"></i><strong>{{running}}</strong><div>{{#str}} running, local_customerintel {{/str}}</div></div>
    <div class="custintel_card"><i class="fa-thin fa-circle-check" style="color: #0BA0A9"></i><strong>{{completed}}</strong><div>{{#str}} completed, local_customerintel {{/str}}</div></div>
    <div class="custintel_card"><i class="fa-thin fa-warning" style="color: #F4604B"></i><strong>{{failed}}</strong><div>{{#str}} failed, local_customerintel {{/str}}</div></div>
  </div>
  <div class="card mb-3">
    <!--div class="card-header">{{#str}} recentruns, local_customerintel {{/str}}</div-->

    <div style="margin-top: 20px;">

    <div class="intel_dash_search">
          <form action="" method="GET">
          <input type="search" placeholder="Search by ID, Company or Date..." name="intel" value="{{search}}" class="customer_intel_searchfield" style="width: 48%" data-initial-value="">
          <button type="submit" style="align-self: flex-end;">
            <i class="fa-light fa-magnifying-glass"></i>
          </button>
          </form>
      <a class="btn btn-primary" style="float: right" href="{{runurl}}"> <i class="fa-light fa-circle-plus"></i> {{#str}} newanalysisrun, local_customerintel {{/str}}</a>
    </div>

        <table class="inteldash_table">
          <thead><tr><th>ID</th><th>{{#str}} customer, local_customerintel {{/str}}</th><th>{{#str}} target, local_customerintel {{/str}}</th><th>{{#str}} status, local_customerintel {{/str}}</th><th>{{#str}} started, local_customerintel {{/str}}</th><th>{{#str}} completed, local_customerintel {{/str}}</th><th>Actions</th></tr></thead>
          <tbody>
            {{#runs}}
            <tr>
              <td style="color: #969696">{{id}}</td>
              <td>{{customername}}</td>
              <td style="color: #969696">{{targetname}}</td>
              <td class="icon_cell">{{{icon}}}</td>
              <td style="color: #969696">{{timestarted_h}}</td>
              <td style="color: #969696">{{timecompleted_h}}</td>
              <td>
                {{#can_view_report}}
                  <a href="{{report_url}}" class="btn btn-sm cancelbutton">

                    {{#is_processing}}

                        <i class="fa-solid fa-circle-notch fa-spin"></i> In Progress

                    {{/is_processing}}
                      {{^is_processing}}
                           <i class="fa-light fa-eye"></i> View Report
                      {{/is_processing}}

                  </a>
                {{/can_view_report}}
              </td>
            </tr>
            {{/runs}}
          </tbody>
        </table>

      {{^runs}}
        <em>{{#str}} noruns, local_customerintel {{/str}}</em>
      {{/runs}}
    </div>
  </div>
</div>
