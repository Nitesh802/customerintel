<style>

body #page.drawers .main-inner {width: 100% !important; max-width:100% !important; padding: 0 !important;}
#customerintel_head {width: 100%;  background: url(https://sales.multi.rubi.digital/pix/hero-head-bg.png)!important; overflow: visible; margin: 0; }
#inner_customerintel_head {margin: 0 auto; max-width: 1200px; color: #fff;display: flex;padding: 20px 0}
#inner_customerintel_head h3 {color: #fff;line-height:100px; verical-align: middle;margin-left: 12px;}
.local_customerintel {max-width: 1200px; margin: 0 auto;}
.intelcard_row {display: flex; gap: 8px; width: 100%;}
.custintel_card {background: #fff; padding: 20px;font-size: 15px; border-radius: 8px;width: 25%}
.custintel_card strong {font-size: 19px;}
.custintel_card i {font-size: 42px; float: right; display: inline;}
.intel_dash_search {width: 100%; padding-bottom: 20px;border-bottom: solid 1px #C4C4C4;display: block; clear: both; height: 70px;}
.intel_dash_search .btn {}
.inteldash_table {margin-top: 20px;}
.inteldash_table thead {margin-top: 20px;}
.inteldash_table {width: 100%;border-collapse: separate;border-spacing: 0 8px; }
.inteldash_table tbody tr{background: #fff;}
.inteldash_table tbody tr:hover{background: #F9FAFC;}
.inteldash_table tbody td, .inteldash_table thead th {padding: 10px;}
.inteldash_table td.icon_cell {text-align: center;}
.inteldash_table td:first-child{border-top-left-radius: 8px;border-bottom-left-radius: 8px; }
.inteldash_table td:last-child{border-top-right-radius: 8px;border-bottom-right-radius: 8px;}
.dropdown {float: right;}
.dropdown-toggle {float: right; border: none; background: none; font-size: 22px;}
.cancelbutton {font-weight: 400!important;}
#page-header, .secondary-navigation, .dropdown-toggle::after {display: none;}

i.processing, i.running,i.completed {color:#21DBBA }


.intel_dash_search input {

      border: solid 1px #D6D6D6;
    color: #3c3c3c;
    padding: 0 5px;
    font-size: 15px;
    border-radius: 5px;
    height: 44px;
    align-self: flex-end;
    margin: auto 0;
}
.intel_dash_search input::placeholder {

  color:  #D6D6D6;
}

.intel_dash_search button {
color: #3577AF;
    font-size: 15px;
    white-space: nowrap;
    align-self: flex-end;
    height: 28px;
    line-height: 32px;
    margin-top: -4px;
    vertical-align: middle;
    width: auto;
    border: none !important;
    background: transparent;
    margin-left: -40px;
}
@media (max-width: 1200px) {
.inteldash_table,  {width: 95%; margin: 0px auto;}
.intelcard_row {padding: 0 20px;}
.intel_dash_search .btn {margin-right: 20px;}
}

@media (max-width: 800px) {
   .intelcard_row {flex-wrap: wrap;}
   .custintel_card {width: 49%;}
.inteldash_table {padding: 0 20px;}
.inteldash_table th:nth-of-type(1), .inteldash_table th:nth-of-type(3), .inteldash_table th:nth-of-type(5), .inteldash_table th:nth-of-type(6){display: none;}
.inteldash_table td:nth-of-type(1), .inteldash_table td:nth-of-type(3), .inteldash_table td:nth-of-type(5), .inteldash_table td:nth-of-type(6){display: none;}
.inteldash_table th:nth-of-type(4) {text-align: center;}


}
@media (max-width: 600px) {

#inner_customerintel_head svg{display: none;}
#inner_customerintel_head {padding: 20px;}
}
</style>

<header id="customerintel_head">
 <div id="inner_customerintel_head">
