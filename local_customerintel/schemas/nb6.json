{
  "type": "object",
  "required": ["digital_maturity", "technology_stack", "data_capabilities", "cybersecurity", "innovation", "citations"],
  "properties": {
    "digital_maturity": {
      "type": "object",
      "required": ["overall_score", "assessment", "key_strengths", "gaps"],
      "properties": {
        "overall_score": {
          "type": "string",
          "enum": ["leader", "advanced", "developing", "lagging"]
        },
        "assessment": {"type": "string"},
        "key_strengths": {
          "type": "array",
          "items": {"type": "string"}
        },
        "gaps": {
          "type": "array",
          "items": {"type": "string"}
        }
      }
    },
    "technology_stack": {
      "type": "object",
      "required": ["core_systems", "cloud_adoption", "modernization_status"],
      "properties": {
        "core_systems": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["system", "vendor", "status"],
            "properties": {
              "system": {"type": "string"},
              "vendor": {"type": "string"},
              "status": {
                "type": "string",
                "enum": ["modern", "stable", "legacy", "replacing"]
              }
            }
          }
        },
        "cloud_adoption": {
          "type": "object",
          "required": ["level", "providers", "strategy"],
          "properties": {
            "level": {"type": "string"},
            "providers": {
              "type": "array",
              "items": {"type": "string"}
            },
            "strategy": {"type": "string"}
          }
        },
        "modernization_status": {"type": "string"}
      }
    },
    "data_capabilities": {
      "type": "object",
      "required": ["analytics_maturity", "ai_ml_adoption", "data_governance"],
      "properties": {
        "analytics_maturity": {
          "type": "string",
          "enum": ["predictive", "diagnostic", "descriptive", "basic"]
        },
        "ai_ml_adoption": {
          "type": "object",
          "required": ["level", "use_cases"],
          "properties": {
            "level": {
              "type": "string",
              "enum": ["extensive", "moderate", "pilot", "none"]
            },
            "use_cases": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "data_governance": {
          "type": "string",
          "enum": ["mature", "developing", "basic", "none"]
        }
      }
    },
    "cybersecurity": {
      "type": "object",
      "required": ["posture", "recent_incidents", "investments"],
      "properties": {
        "posture": {
          "type": "string",
          "enum": ["robust", "adequate", "developing", "vulnerable"]
        },
        "recent_incidents": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["incident", "impact"],
            "properties": {
              "incident": {"type": "string"},
              "impact": {"type": "string"},
              "date": {"type": "string"}
            }
          }
        },
        "investments": {"type": "string"}
      }
    },
    "innovation": {
      "type": "object",
      "required": ["r_d_focus", "patents", "innovation_labs"],
      "properties": {
        "r_d_focus": {
          "type": "array",
          "items": {"type": "string"}
        },
        "patents": {
          "type": "object",
          "properties": {
            "filed": {"type": "integer"},
            "granted": {"type": "integer"},
            "key_areas": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        },
        "innovation_labs": {"type": "boolean"}
      }
    },
    "citations": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["source_id"],
        "properties": {
          "source_id": {"type": "integer"},
          "quote": {"type": "string"},
          "page": {"type": "string"},
          "url": {"type": "string"}
        }
      }
    }
  }
}